            <profile id="30cbafed-47c1-435b-b743-c79f9dfc4c52" name="3) Psychic Barrier" hidden="false" typeId="e743f6ce-6ef0-4d8b-a0a2-cf98d6a3150b" typeName="Psychic Power">
