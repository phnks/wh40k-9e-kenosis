    <profile id="dc3bf0ea-59ed-4bc1-adbc-2c9a46673d4d" name="Master of Machines" publicationId="629ea54e-0a94-4d10-887c-342cb4a4c74f" hidden="false" typeId="d8b7eb23-4e11-4efa-90fb-b26e0ab318fa" typeName="Abilities">
