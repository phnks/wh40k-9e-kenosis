    <profile id="778ae7c6-b525-4f8d-bd4e-c641f01b4acb" name="Will of Asuryan (Blessing)" publicationId="1f62-db93-63f4-4dc7" page="122" hidden="false" typeId="ae70-4738-0161-bec0" typeName="Psychic Power">
