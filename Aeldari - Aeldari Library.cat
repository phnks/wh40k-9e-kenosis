    <profile id="9a7233f8-a438-4892-9780-b904493ed5e6" name="Executioner (Witchfire)" publicationId="1f62-db93-63f4-4dc7" page="122" hidden="false" typeId="ae70-4738-0161-bec0" typeName="Psychic Power">
